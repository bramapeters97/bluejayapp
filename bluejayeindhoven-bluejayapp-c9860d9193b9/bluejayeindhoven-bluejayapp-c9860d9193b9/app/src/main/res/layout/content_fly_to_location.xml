<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".FlyToLocation">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:descendantFocusability="beforeDescendants"
        android:focusableInTouchMode="true"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <include
            layout="@layout/app_bar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="visible" />

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/onlineButton3"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="2dp"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="@+id/textView"
                app:layout_constraintStart_toEndOf="@+id/textView"
                app:layout_constraintTop_toTopOf="@+id/textView"
                app:srcCompat="@android:drawable/presence_online" />

            <ImageView
                android:id="@+id/offlineButton3"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="2dp"
                app:layout_constraintBottom_toBottomOf="@+id/textView"
                app:layout_constraintStart_toEndOf="@+id/textView"
                app:layout_constraintTop_toTopOf="@+id/textView"
                app:srcCompat="@android:drawable/presence_busy" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:fontFamily="@font/roboto_light"
                android:text="Flight Commands"
                android:textAlignment="center"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </android.support.constraint.ConstraintLayout>

        <android.support.v7.widget.CardView
            android:id="@+id/cardView3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="15dp">

            <android.support.constraint.ConstraintLayout
                android:id="@+id/constraintLayout2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:descendantFocusability="beforeDescendants"
                android:focusableInTouchMode="true">

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="8dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <android.support.constraint.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/localisation_z"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_thin"
                            android:text="..."
                            android:textSize="20sp"
                            app:layout_constraintStart_toEndOf="@+id/z"
                            app:layout_constraintTop_toTopOf="@+id/z" />

                        <TextView
                            android:id="@+id/localisation_y"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_thin"
                            android:text="..."
                            android:textSize="20sp"
                            app:layout_constraintStart_toEndOf="@+id/y"
                            app:layout_constraintTop_toTopOf="@+id/y" />

                        <TextView
                            android:id="@+id/localisation_x"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_thin"
                            android:text="..."
                            android:textSize="20sp"
                            app:layout_constraintStart_toEndOf="@+id/x"
                            app:layout_constraintTop_toTopOf="@+id/x" />

                        <TextView
                            android:id="@+id/y"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="60dp"
                            android:fontFamily="@font/roboto_thin"
                            android:text="y: "
                            android:textSize="20sp"
                            android:textStyle="bold|italic"
                            app:layout_constraintStart_toStartOf="@+id/x"
                            app:layout_constraintTop_toTopOf="@+id/localisation_x" />

                        <TextView
                            android:id="@+id/x"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="16dp"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/roboto_thin"
                            android:text="x: "
                            android:textSize="20sp"
                            android:textStyle="bold|italic"
                            app:layout_constraintStart_toEndOf="@+id/textView43"
                            app:layout_constraintTop_toTopOf="@+id/textView42" />

                        <TextView
                            android:id="@+id/z"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="130dp"
                            android:fontFamily="@font/roboto_thin"
                            android:text="z: "
                            android:textSize="20sp"
                            android:textStyle="bold|italic"
                            app:layout_constraintStart_toStartOf="@+id/x"
                            app:layout_constraintTop_toTopOf="@+id/localisation_y" />

                        <TextView
                            android:id="@+id/textView42"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Localisation"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/textView43"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginBottom="8dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Live position:"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView42" />
                    </android.support.constraint.ConstraintLayout>

                </android.support.v7.widget.CardView>

                <ScrollView
                    android:id="@+id/scrollView2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <android.support.constraint.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:descendantFocusability="beforeDescendants"
                        android:focusableInTouchMode="true">

                        <TextView
                            android:id="@+id/textView9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="12dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Commands"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="18sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/divider8" />

                        <TextView
                            android:id="@+id/textView10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_light"
                            android:text="Only work in offboard-mode"
                            app:layout_constraintStart_toStartOf="@+id/textView9"
                            app:layout_constraintTop_toBottomOf="@+id/textView9" />

                        <ImageButton
                            android:id="@+id/rebootButton2"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_margin="10dp"
                            android:layout_marginTop="8dp"
                            android:src="@android:drawable/stat_notify_sync_noanim"
                            android:text="Button"
                            android:tint="@color/common_google_signin_btn_text_dark_focused"
                            app:layout_constraintEnd_toStartOf="@+id/powerOffButton2"
                            app:layout_constraintTop_toBottomOf="@+id/changeBtn2"
                            tools:src="@android:drawable/stat_notify_sync_noanim" />

                        <ImageButton
                            android:id="@+id/powerOffButton2"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_margin="10dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="8dp"
                            android:src="@android:drawable/ic_lock_power_off"
                            android:text="Button"
                            android:tint="@color/common_google_signin_btn_text_dark_focused"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/changeBtn2"
                            tools:src="@android:drawable/ic_lock_power_off" />

                        <Button
                            android:id="@+id/changeBtn2"
                            android:layout_width="110dp"
                            android:layout_height="50sp"
                            android:layout_marginTop="12dp"
                            android:layout_marginEnd="9dp"
                            android:fontFamily="@font/roboto"
                            android:text="change"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/mapImageFlyTo" />

                        <Button
                            android:id="@+id/stopButton"
                            android:layout_width="110dp"
                            android:layout_height="45dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginEnd="10dp"
                            android:fontFamily="@font/roboto"
                            android:text="STOP"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView37" />

                        <Button
                            android:id="@+id/landButton"
                            android:layout_width="110dp"
                            android:layout_height="45dp"
                            android:layout_marginTop="12dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="12dp"
                            android:fontFamily="@font/roboto"
                            android:text="LAND"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView44" />

                        <TextView
                            android:id="@+id/textView7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="14dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Drone:"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="18sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/mapImageFlyTo" />

                        <TextView
                            android:id="@+id/textView8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="20dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Battery:"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView4" />

                        <TextView
                            android:id="@+id/textView17"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Take-Off"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="@+id/textView20"
                            app:layout_constraintTop_toBottomOf="@+id/textView10" />

                        <TextView
                            android:id="@+id/textView36"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="14dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Fly To"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="@+id/textView20"
                            app:layout_constraintTop_toBottomOf="@+id/textView20" />

                        <TextView
                            android:id="@+id/textView41"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="14dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Stop"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="@+id/textView20"
                            app:layout_constraintTop_toBottomOf="@+id/textView37" />

                        <TextView
                            android:id="@+id/textView47"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="14dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Land"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="@+id/textView20"
                            app:layout_constraintTop_toBottomOf="@+id/textView44" />

                        <TextView
                            android:id="@+id/textView19"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_light"
                            android:text="Flight Status:"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Subtitle"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="@+id/textView8"
                            app:layout_constraintTop_toBottomOf="@+id/textView8" />

                        <TextView
                            android:id="@+id/textView4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="IP-Address:"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView7" />

                        <TextView
                            android:id="@+id/textView20"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:fontFamily="@font/roboto_light"
                            android:text="Set height in mm:"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView17" />

                        <TextView
                            android:id="@+id/textView37"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_light"
                            android:text="New position in m:"
                            app:layout_constraintStart_toStartOf="@+id/textView36"
                            app:layout_constraintTop_toBottomOf="@+id/textView36" />

                        <TextView
                            android:id="@+id/textView44"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_light"
                            android:text="Hover above current position"
                            app:layout_constraintStart_toStartOf="@+id/textView41"
                            app:layout_constraintTop_toBottomOf="@+id/textView41" />

                        <TextView
                            android:id="@+id/textView48"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_light"
                            android:text="Landing at current position"
                            app:layout_constraintStart_toStartOf="@+id/textView41"
                            app:layout_constraintTop_toBottomOf="@+id/textView47" />

                        <TextView
                            android:id="@+id/droneid2"
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="14dp"
                            android:fontFamily="@font/roboto_thin"
                            android:text="drone"
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Title"
                            android:textSize="18sp"
                            android:textStyle="italic"
                            app:layout_constraintStart_toStartOf="@+id/batteryLife"
                            app:layout_constraintTop_toBottomOf="@+id/mapImageFlyTo" />

                        <TextView
                            android:id="@+id/flightStatus"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:fontFamily="@font/roboto_thin"
                            android:text="Loading..."
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Title"
                            android:textSize="14sp"
                            app:layout_constraintStart_toEndOf="@+id/textView19"
                            app:layout_constraintTop_toBottomOf="@+id/batteryLife" />

                        <TextView
                            android:id="@+id/batteryLife"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_thin"
                            android:text="Loading..."
                            android:textAppearance="@style/TextAppearance.AppCompat.Light.SearchResult.Title"
                            android:textSize="14sp"
                            app:layout_constraintStart_toStartOf="@+id/flightStatus"
                            app:layout_constraintTop_toTopOf="@+id/textView8" />

                        <TextView
                            android:id="@+id/ipAddress2"
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/roboto_thin"
                            android:text="ip-address"
                            android:textStyle="italic"
                            app:layout_constraintStart_toStartOf="@+id/batteryLife"
                            app:layout_constraintTop_toBottomOf="@+id/textView7" />

                        <ImageView
                            android:id="@+id/mapImageFlyTo"
                            android:layout_width="0dp"
                            android:layout_height="130dp"
                            android:radius="10dp"
                            android:scaleType="centerCrop"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:srcCompat="@mipmap/map_example"></ImageView>

                        <View
                            android:id="@+id/divider8"
                            android:layout_width="0dp"
                            android:layout_height="1dp"
                            android:layout_marginStart="15dp"
                            android:layout_marginTop="18dp"
                            android:layout_marginEnd="10dp"
                            android:background="?android:attr/listDivider"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView19" />

                        <EditText
                            android:id="@+id/heightInput"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="14dp"
                            android:ems="10"
                            android:fontFamily="@font/roboto_light"
                            android:hint="height"
                            android:inputType="numberDecimal"
                            android:textAlignment="center"
                            android:textStyle="italic"
                            app:layout_constraintEnd_toEndOf="@+id/constraintLayout3"
                            app:layout_constraintTop_toBottomOf="@+id/textView10" />

                        <ImageButton
                            android:id="@+id/takeOffButton2"
                            android:layout_width="wrap_content"
                            android:layout_height="50dp"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="10dp"
                            android:tint="@color/common_google_signin_btn_text_light_focused"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/textView10"
                            app:srcCompat="@drawable/ic_menu_send"
                            tools:srcCompat="@drawable/ic_menu_send" />

                        <ImageButton
                            android:id="@+id/flyToButton"
                            android:layout_width="wrap_content"
                            android:layout_height="50dp"
                            android:layout_marginEnd="10dp"
                            android:tint="@color/common_google_signin_btn_text_light_focused"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/takeOffButton2"
                            app:srcCompat="@drawable/ic_menu_send"
                            tools:srcCompat="@drawable/ic_menu_send" />

                        <android.support.constraint.ConstraintLayout
                            android:id="@+id/constraintLayout3"
                            android:layout_width="wrap_content"
                            android:layout_height="39dp"
                            android:layout_marginEnd="14dp"
                            app:layout_constraintEnd_toStartOf="@+id/takeOffButton2"
                            app:layout_constraintTop_toTopOf="@+id/textView36">

                            <TextView
                                android:id="@+id/textView39"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/roboto_light"
                                android:text="("
                                android:textSize="26sp"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <TextView
                                android:id="@+id/textView40"
                                android:layout_width="wrap_content"
                                android:layout_height="0dp"
                                android:fontFamily="@font/roboto_light"
                                android:text=")"
                                android:textSize="26sp"
                                app:layout_constraintStart_toEndOf="@+id/z_coordinate"
                                app:layout_constraintTop_toTopOf="parent" />

                            <EditText
                                android:id="@+id/x_coordinate"
                                android:layout_width="35dp"
                                android:layout_height="wrap_content"
                                android:ems="10"
                                android:fontFamily="@font/roboto_light"
                                android:hint="x"
                                android:inputType="numberDecimal"
                                android:textAlignment="center"
                                android:textStyle="italic"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toEndOf="@+id/textView39"
                                app:layout_constraintTop_toTopOf="parent" />

                            <EditText
                                android:id="@+id/y_coordinate"
                                android:layout_width="35dp"
                                android:layout_height="wrap_content"
                                android:ems="10"
                                android:fontFamily="@font/roboto_light"
                                android:inputType="numberDecimal"
                                android:hint="y"
                                android:textAlignment="center"
                                android:textStyle="italic"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toEndOf="@+id/x_coordinate"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintVertical_bias="0.0" />

                            <EditText
                                android:id="@+id/z_coordinate"
                                android:layout_width="35dp"
                                android:layout_height="wrap_content"
                                android:inputType="numberDecimal"
                                android:ems="10"
                                android:fontFamily="@font/roboto_light"
                                android:hint="z"
                                android:textAlignment="center"
                                android:textStyle="italic"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toEndOf="@+id/y_coordinate"
                                app:layout_constraintTop_toTopOf="parent" />

                        </android.support.constraint.ConstraintLayout>

                        <ImageView
                            android:id="@+id/questionMark"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:background="#005A5A5A"
                            app:layout_constraintBottom_toBottomOf="@+id/textView36"
                            app:layout_constraintStart_toEndOf="@+id/textView36"
                            app:layout_constraintTop_toTopOf="@+id/textView36"
                            app:srcCompat="@android:drawable/ic_menu_help" />

                        <ImageView
                            android:id="@+id/questionMark3"
                            android:layout_width="wrap_content"
                            android:layout_height="0dp"
                            android:layout_marginTop="2dp"
                            android:layout_marginBottom="2dp"
                            android:background="#005A5A5A"
                            app:layout_constraintBottom_toBottomOf="@+id/textView9"
                            app:layout_constraintStart_toEndOf="@+id/textView9"
                            app:layout_constraintTop_toTopOf="@+id/textView9"
                            app:srcCompat="@android:drawable/ic_menu_help" />

                    </android.support.constraint.ConstraintLayout>
